-- –°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –∏–≥—Ä—ã –Ω–∞ —Ä–∞–±–æ—á–∏–π —Å—Ç–æ–ª
local desktop = os.getenv("USERPROFILE") .. "\\Desktop\\"
local filename = desktop .. game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name .. "_" .. game.PlaceId .. ".rbxl"

-- –ü—Ä–æ–≤–µ—Ä—è–µ–º –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ—Ç –ª–∏ executor saveinstance
if saveinstance then
    saveinstance(nil, filename)
    print("‚úÖ –ò–≥—Ä–∞ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∞ –Ω–∞ —Ä–∞–±–æ—á–∏–π —Å—Ç–æ–ª!")
    print("üìÅ –§–∞–π–ª: " .. filename)
    
    -- –û—Ç–∫—Ä—ã–≤–∞–µ–º –ø–∞–ø–∫—É —Å —Ñ–∞–π–ª–æ–º (—Ä–∞–±–æ—Ç–∞–µ—Ç –Ω–∞ Windows)
    if setclipboard then
        setclipboard(filename)
        print("üìã –ü—É—Ç—å —Å–∫–æ–ø–∏—Ä–æ–≤–∞–Ω –≤ –±—É—Ñ–µ—Ä –æ–±–º–µ–Ω–∞")
    end
else
    print("‚ùå –í–∞—à executor –Ω–µ –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ—Ç saveinstance!")
    print("–ü–æ–ø—Ä–æ–±—É–π—Ç–µ –¥—Ä—É–≥–∏–µ –º–µ—Ç–æ–¥—ã:")
    
    -- –ê–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–∞ —á–µ—Ä–µ–∑ GetObjects
    local success, data = pcall(function()
        return game:GetService("HttpService"):JSONEncode(game:GetObjects())
    end)
    
    if success then
        writefile(desktop .. "game_backup.txt", data)
        print("üìÑ –î–∞–Ω–Ω—ã–µ –∏–≥—Ä—ã —Å–æ—Ö—Ä–∞–Ω–µ–Ω—ã –∫–∞–∫ —Ç–µ–∫—Å—Ç–æ–≤—ã–π —Ñ–∞–π–ª")
    end
end 
